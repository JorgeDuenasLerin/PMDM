<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".U5054_Permisos">

    <EditText
        android:id="@+id/u5054_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/u5054_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/u5054_text"
        app:layout_constraintStart_toStartOf="@+id/u5054_text"
        app:layout_constraintTop_toBottomOf="@+id/u5054_text" />

    <Button
        android:id="@+id/u5054_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Escribe"
        app:layout_constraintEnd_toEndOf="@+id/u5054_number"
        app:layout_constraintStart_toStartOf="@+id/u5054_number"
        app:layout_constraintTop_toBottomOf="@+id/u5054_number" />
</androidx.constraintlayout.widget.ConstraintLayout>